<Window x:Class="lection_example_24_dec_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lection_example_24_dec_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel ButtonBase.Click="OnClick">
            <Label ButtonBase.Click="Label_Click">
                <Button Name="YesButton" MouseDoubleClick="YesButton_MouseDoubleClick"></Button>
            </Label>
            <Button Name="YesBUtton" Click="YesButton_Click">Yes</Button>
        </StackPanel>
    </Grid>
</Window>
